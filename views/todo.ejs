<html>
  <HEAD>
  	<TITLE>
  		ToDo List
  	</TITLE>
    <script src="/assets/jquery-1.12.3.min.js">
    </script>
  <script src="/assets/todo-list.js"></script>
  <link href="/assets/style.css" rel="stylesheet" type="text/css"/>
  </HEAD>
  <BODY>
  <h1>My Todo List</h1>
  	<div id="todo-table">
  		<FORM>
  			<input type="text" name="item" placeholder="Add New Item...." required />
  			<button type="submit">
  				Add Item
  			</button>
  		</FORM>
  		<ul>
  		<% for(var i=0; i < todos.length ; i++){ %>
  			<li><%= todos[i].item %></li>
  		<% } %>
  		</ul>
  	</div>
  </BODY>

</html>